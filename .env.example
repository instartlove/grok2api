# Example environment variables for grok2api
# Copy this file to `.env` and fill in real values. `.env` is already gitignored.

# Required secrets
API_KEY=CHANGE_ME
ADMINPASSWORD=CHANGE_ME

# Proxy settings (example format)
PROXY=http://proxy_user:proxy_password@host:port
DYNAMIC_PROXY_API=https://proxy.example.com/random/us?protocol=http&host=us.proxy302.com&api_key=CHANGE_ME

# Proxy validation and retries
PROXY_RETRY_LIMIT=20
PROXY_VALIDATE_URL=https://grok.com/
PROXY_VALIDATE_TIMEOUT=15

# Request timeout (seconds)
REQUEST_TIMEOUT=180

# Feature toggles
IS_TEMP_CONVERSATION=true
SHOW_SEARCH_RESULTS=true
SHOW_THINKING=true
DISABLE_DYNAMIC_HEADERS=true
MANAGER_SWITCH=true
